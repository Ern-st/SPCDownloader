{
  "manifest_version": 2,

  "name": "Seagate Downloader",
  "description": "Manage downloads to your Seagate Personal Cloud NAS",
  "version": "1.0",

  "browser_action": {
      "default_title": "Seagate Downloader",
      "default_popup": "options.html"
    },

  "icons": {
          "16": "assets/img/16.png",
          "48": "assets/img/48.png",
          "128": "assets/img/128.png"
        },

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "permissions": [
    "activeTab",
    "notifications",
    "contextMenus",
    "storage",
    "tabs"
  ],
  "background": {"page": "background.html"}
}